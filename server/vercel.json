{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/server/api/auth/$1"
    },
    {
      "src": "/api/private/(.*)",
      "dest": "/server/api/private/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/$1"
    }
  ]
}

